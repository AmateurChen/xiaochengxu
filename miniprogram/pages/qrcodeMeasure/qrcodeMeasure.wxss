.qrcode_info_error{
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
}

.qrcode_info_container {
  display: flex;
  flex: 1;
  margin: 5rpx;
  flex-direction: column;
}

.qrcode_info_scroll_container {
  display: flex;
  flex: 1;
  flex-direction: column;
  margin-bottom: 20rpx;
}

.qrcode_info_tbName_input {
  height: 100rpx;
  width: 100%;
  text-align: center;
  font-size: 45rpx;
  border-bottom: solid #888 1rpx;
}

.qrcode_info_list_item_container {
  display: flex;
  flex: 1;
  min-height: 120rpx;
  align-items: center;
  justify-content: flex-end;
  background: rgb(235, 235, 235);
  padding-left: 30rpx;
  padding-right: 30rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
  border-bottom: solid #888 1rpx;
}
.qrcode_info_list_item_container text {
  display: flex;
  flex: 1;
}
.qrcode_info_list_item_container image {
  width: 40rpx;
  height: 40rpx;
}

.qrcode_info_list_gone_container {
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: center;
  padding-left: 30rpx;
  padding-right: 30rpx;
}



.qrcode_info_list_child_container {
  display: flex;
  flex: 1;
  flex-direction: column;
  border: dashed #888 1rpx;
  border-radius: 10rpx;
  margin-bottom: 10rpx;
  margin-top: 10rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.qrcode_info_list_child_delete {
  height: 60rpx;
  width: 60rpx;
  background-color: rgb(230, 230, 230);
  margin-bottom: -40rpx;
  margin-left: -20rpx;
  z-index: 99;
}

.qrcode_info_list_child_container input{
  height: 80rpx;
  width: 100%;
  text-align: right;
  border-bottom: dashed #888 1rpx;
}


.qrcode_info_container button{
  width: 100%;
  background-color: #d81e06;
  color: white;
  margin-bottom: 20rpx;

}

.qrcode_info_list_gone_container button{
  width: 100%;
  background: rgb(240, 240, 240);
  color: #888;
  margin-top: 10rpx;
}

.qrcode_info_tester_item_container {
  display: flex;
  flex: 1;
  flex-direction: row;
  align-items: center;
}

.qrcode_info_tester_item_container image {
  height: 60rpx;
  width: 60rpx;
}


.modal-measure-container {
  display: flex;
  flex: 1;
  max-height: 500rpx;
  flex-direction: column;
}
.modal-measure-container input {
  width: 100%;
  height: 80rpx;
  text-align: center;
  border-bottom: dashed #888 1rpx;
}

.modal-measure-container view {
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: white;
  border-radius: 10rpx;
  margin-top: 30rpx;
}

.modal-measure-container view text {
  display: flex;
  flex: 1;
  width: 90%;
  height: 80rpx;
  align-items: center;
  justify-content: center;
  border-bottom: solid #888 1rpx;
}

