.home_container {
  display: flex;
  flex: 1;
  flex-direction: column;
}

.user_component {
  display: flex;
  height: 350rpx;
  background-color: #d81e06;
}
.home_component{
  flex: 1;
  margin-left: 80rpx;
  margin-right: 80rpx;
}

.home_bottom_container {
  display: flex;
  width: 100%;
  height: 120rpx;
  position: relative;
  bottom: 0rpx;
  align-items: center;
  background-color: white;
}

.home_bottom_container image {
  height: 80rpx;
  width: 100%;
  margin-left: 100rpx;
  margin-right: 100rpx;
  line-height: 100rpx;
  text-align: center;
}

.home_bottom_container text {
  flex: 1;
  font-size: 50rpx;
  text-align: center;
}